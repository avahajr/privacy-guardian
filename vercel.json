{
  "builds": [
    {
      "src": "api/server.py",
      "use": "@vercel/python",
      "config": {
        "includeFiles": [
          "api/"
        ]
      }
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/server.py"
    }
  ],
  "buildCommand": "npm install && npm run build"
}